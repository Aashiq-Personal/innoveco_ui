    "@hookform/resolvers": "^3.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "daisyui": "^3.7.3",
    "dayjs": "^1.11.7",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "web-vitals": "^2.1.4",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-merge": "^5.9.0",
    "zod": "^3.22.2"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack serve --mode development --config webpack/webpack.dev.js",
    "winstart": "webpack serve --mode development --config webpack/webpack.dev.js",
    "start:clean": "rm -rf node_modules && npm i && npm start",
    "cleanup": "rm -rf dist",
    "postbuild": "cp public/manifest.json dist/manifest.json",
    "build:prod": "npm run cleanup && webpack --mode production --bail --config webpack/webpack.prod.js && npm run postbuild",
    "analyze": "webpack --mode development --bail --config webpack/webpack.analyzer.js",
    "test": "jest",
    "test:ci": "jest --ci",
    "eslint": "eslint --fix .",
    "lintfix": "lint-staged",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "webpack-cli": "^5.1.4"
  }
}
